{
	"description":"How many total sentences are in speeches given by female politicians",
	"aggregation": {
		"group_by":["person.sex"],
		"order_by":[]
	},
	"filtering": {
		"columns":["person.sex", "SUM(speech.sentence_count) AS total_sentences"],
		"conditions": [
			{
				"column":"person.sex",
				"operator":"=",
				"value":"'F'"
			}
		],
		"limit":""
	}
}
