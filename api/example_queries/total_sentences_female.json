{
	"aggregation": {
		"tables":["speech", "person"],
		"joins": [
			{
				"type":"inner",
				"left_table":"speech",
				"left_column":"author_id",
				"right_table":"person",
				"right_column":"person_id"
			}
		],
		"group_by":[],
		"order_by":[]
	},
	"filtering": {
		"columns":["SUM(speech.sentence_count) AS total_sentences"],
		"conditions": [
			{
				"column":"person.sex",
				"operator":"=",
				"value":"F"
			}
		]
	}
}
