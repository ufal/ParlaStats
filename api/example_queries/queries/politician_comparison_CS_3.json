{
	"target_databases":["databaseCS"],
	"description":"Comparison of total speeches by Adam Kalous and Adam Rykala",
	"aggregation":{
		"group_by":["persName.forename", "persName.surname", "person.person_id"],
		"order_by":[]		
	},
	"filtering": {
		"columns":["person.person_id", "persName.forename", "persName.surname", "person.person_id", "COUNT(speech.id) AS total_speeches"],
		"conditions": [
			{
				"column":"persName.forename",
				"operator":" IN ",
				"value":"('Adam')"
			},
			{
				"column":"persName.surname",
				"operator":" IN ",
				"value":"('Kalous', 'Rykala')"
			}
		],
		"limit":""
	}
}
