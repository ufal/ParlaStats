{
	"target_databases":["databaseCS", "databaseLV"],
	"description": "How many speeches did male politicians give between 1.1.2019 and 1.1.2020",
	"aggregation": {
		"group_by":["person.sex"],
		"order_by":[]
	},
	"filtering": {
		"columns":["person.sex", "COUNT(speech.id) AS total_speeches"],
		"conditions":[
			{
				"column":"person.sex",
				"operator":"=",
				"value":"'M'"
			},
			{
				"column":"speech.date",
				"operator":">",
				"value":"'2019-01-01'"
			},
			{
				"column":"speech.date",
				"operator":"<",
				"value":"'2020-01-01'"
			}
		],
		"limit":""
	}
}
