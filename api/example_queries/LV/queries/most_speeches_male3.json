{
	"description":"TOP 10 male speakers with most_speeches",
	"aggregation": {
		"group_by":["persName.forename", "persName.surname"],
		"order_by":[
			{
				"column":"total_speeches",
				"direction":"DESC"
			}
		]
	},
	"filtering": {
		"columns":["persName.forename", "persName.surname", "COUNT(speech.id) AS total_speeches"],
		"conditions": [
			{
				"column":"person.sex",
				"operator":"=",
				"value":"'M'"
			}
		],
		"limit":"10"
	}
}
